{
  "name": "Loki Autofill",
  "version": "1.0",
  "description": "Chrome extension for autofilling commonly used Loki queries",
  "manifest_version": 1,
  "icons": {
    "16": "icons/loki.png",
    "48": "icons/loki.png",
    "128": "icons/loki.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/loki.png"
  }
}
