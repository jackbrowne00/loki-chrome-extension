{
  "manifest_version": 3,
  "name": "Loki Autofill",
  "version": "1.0",
  "description": "Chrome extension for autofilling commonly used Loki queries",
  "icons": {
    "16": "icons/loki.png",
    "48": "icons/loki.png",
    "128": "icons/loki.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": "icons/loki.png"
  },
  "permissions": ["activeTab", "scripting", "storage", "tabs", "<all_urls>"],
  "author": "Jack Browne"
}
